@page
@model TBSExam.UI.Pages.Mantenimiento.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].cveUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].nombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].usuPwd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].numAcceso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].ultimoAcceso)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Usuario) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cveUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombreUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.usuPwd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numAcceso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ultimoAcceso)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.usuario_id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.usuario_id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.usuario_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
